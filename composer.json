{
    "license": "MIT",
    "type": "project",
    "description": "PHP web application to manage the members of the christian church Evangeliums-Christengemeinde e.V. in Paderborn/Germany",
    "autoload": {
        "psr-4": { "": "src/" },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "require": {
        "php": ">=7.3.0",
        "symfony/symfony": "~4.3.0",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/swiftmailer-bundle": "^3.3.0",
        "symfony/monolog-bundle": "^3.0.2",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.0",

        "doctrine/doctrine-fixtures-bundle": "*",
        "knplabs/knp-paginator-bundle": "~2.4",
        "mopa/bootstrap-bundle": "^3.3.0",
        "naitsirch/tcpdf-extension": "dev-master",
        "phpoffice/phpspreadsheet": "~1.9.0",
        "tecnickcom/tcpdf": "@dev",
        "twbs/bootstrap": ">=3.3,<4.0",
        "twig/extensions": "~1.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "symlink",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
