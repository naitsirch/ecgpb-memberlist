{
    "license": "MIT",
    "type": "project",
    "description": "PHP web application to manage the members of the christian church Evangeliums-Christengemeinde e.V. in Paderborn/Germany",
    "autoload": {
        "psr-4": { "App\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "require": {
        "php": ">=7.3.0",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "knplabs/knp-paginator-bundle": "~2.4",
        "mopa/bootstrap-bundle": "^3.3",
        "naitsirch/tcpdf-extension": "^1.0",
        "phpoffice/phpspreadsheet": "~1.9.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "symfony/flex": "^1.4",
        "symfony/framework-bundle": "^4.4",
        "symfony/asset": "^4.4",
        "symfony/console": "^4.4",
        "symfony/doctrine-bridge": "^4.4",
        "symfony/form": "^4.4",
        "symfony/intl": "^4.4",
        "symfony/security-csrf": "^4.4",
        "symfony/serializer": "^4.4",
        "symfony/templating": "^4.4",
        "symfony/translation": "^4.4",
        "symfony/validator": "^4.4",
        "symfony/yaml": "^4.4",
        "tecnickcom/tcpdf": "@dev",
        "twbs/bootstrap": ">=3.3,<4.0",
        "twig/intl-extra": "^3.0",
        "twig/extra-bundle": "^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "*",
        "symfony/dotenv": "^4.4",
        "symfony/maker-bundle": "^1.14",
        "symfony/profiler-pack": "^1.0"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "scripts": {
        "symfony-scripts": [
            "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "symlink",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
